<h1>Log in</h1>
<%= form_for(:sessions, url: login_path) do |f| %>
    
    <p>
    <%= f.label :username %>
    <%= f.text_field :username %>
    </p>

    <%= f.label :password %>
    <%= f.password_field :password %>
    <p>
    <%= f.submit "Log in" %>
    </p>
    <% end %>
    <% if flash[:alert] %>
        <div class="notice"><%= flash[:alert] %></div>
    <% end %>
    <p>Has not sign up? <%= link_to 'Sign up', register_path(:new) %></p>
